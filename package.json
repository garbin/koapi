{
  "name": "koapi",
  "version": "0.1.1",
  "description": "RESTful API framework based on koajs",
  "main": "lib/koapi.js",
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "scripts": {
    "compile": "babel --presets es2015,stage-0 -d lib/ src/",
    "prepublish": "npm run compile",
    "start": "nodemon -L -x ./node_modules/.bin/babel-node example/app.js",
    "knex": "knex"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garbin/koapi.git"
  },
  "keywords": [
    "RESTful",
    "API",
    "Framework",
    "koa"
  ],
  "author": "Garbin Huang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/garbin/koapi/issues"
  },
  "homepage": "https://github.com/garbin/koapi#readme",
  "dependencies": {
    "bookshelf": "^0.9.2",
    "bookshelf-joi-validator": "^0.2.4",
    "glob": "^7.0.3",
    "knex": "^0.10.0",
    "koa": "^1.2.0",
    "koa-bodyparser": "^2.0.1",
    "koa-bunyan-logger": "^1.3.0",
    "koa-compress": "^1.0.9",
    "koa-cors": "0.0.16",
    "koa-json-error": "^1.0.1",
    "koa-logger": "^1.3.0",
    "koa-ratelimit": "^2.1.0",
    "koa-router": "^5.4.0",
    "koa-static": "^2.0.0",
    "lodash": "^4.6.1",
    "mysql": "^2.10.2"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "nodemon": "^1.9.1"
  }
}
